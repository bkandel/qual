@article{witten_penalized_2009,
	title = {A penalized matrix decomposition, with applications to sparse principal components and canonical correlation analysis},
	volume = {10},
	issn = {1468-4357},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/19377034},
	doi = {10.1093/biostatistics/kxp008},
	abstract = {We present a penalized matrix decomposition {(PMD)}, a new framework for computing a {rank-K} approximation for a matrix. We approximate the matrix X as {circumflexX} = {sigma(k=1)(K)} {d(k)u(k)v(k)(T)}, where d(k), u(k), and v(k) minimize the squared Frobenius norm of X - {circumflexX}, subject to penalties on u(k) and v(k). This results in a regularized version of the singular value decomposition. Of particular interest is the use of L(1)-penalties on u(k) and v(k), which yields a decomposition of X using sparse vectors. We show that when the {PMD} is applied using an L(1)-penalty on v(k) but not on u(k), a method for sparse principal components results. In fact, this yields an efficient algorithm for the {"SCoTLASS"} proposal {(Jolliffe} and others 2003) for obtaining sparse principal components. This method is demonstrated on a publicly available gene expression data set. We also establish connections between the {SCoTLASS} method for sparse principal component analysis and the method of Zou and others (2006). In addition, we show that when the {PMD} is applied to a cross-products matrix, it results in a method for penalized canonical correlation analysis {(CCA).} We apply this penalized {CCA} method to simulated data and to a genomic data set consisting of gene expression and {DNA} copy number measurements on the same set of samples.},
	number = {3},
	journal = {Biostatistics {(Oxford}, England)},
	author = {Witten, Daniela M and Tibshirani, Robert and Hastie, Trevor},
	month = jul,
	year = {2009},
	note = {{PMID:} 19377034},
	keywords = {Algorithms, Biometry, Breast Neoplasms, Chromosomes, Human, Pair 1, Data Interpretation, Statistical, {DNA}, Neoplasm, Female, Gene Dosage, Genomics, Humans, Models, Statistical, Principal Component Analysis},
	pages = {515--534}
},

@article{kurreck_antisense_2003,
	title = {Antisense technologies},
	volume = {270},
	issn = {1432-1033},
	url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1432-1033.2003.03555.x/abstract},
	doi = {10.1046/j.1432-1033.2003.03555.x},
	abstract = {Antisense agents are valuable tools to inhibit the expression of a target gene in a sequence-specific manner, and may be used for functional genomics, target validation and therapeutic purposes. Three types of {anti-mRNA} strategies can be distinguished. Firstly, the use of single stranded antisense-oligonucleotides; secondly, the triggering of {RNA} cleavage through catalytically active oligonucleotides referred to as ribozymes; and thirdly, {RNA} interference induced by small interfering {RNA} molecules. Despite the seemingly simple idea to reduce translation by oligonucleotides complementary to an {mRNA}, several problems have to be overcome for successful application. Accessible sites of the target {RNA} for oligonucleotide binding have to be identified, antisense agents have to be protected against nucleolytic attack, and their cellular uptake and correct intracellular localization have to be achieved. Major disadvantages of commonly used phosphorothioate {DNA} oligonucleotides are their low affinity towards target {RNA} molecules and their toxic side-effects. Some of these problems have been solved in ‘second generation’ nucleotides with alkyl modifications at the 2′ position of the ribose. In recent years valuable progress has been achieved through the development of novel chemically modified nucleotides with improved properties such as enhanced serum stability, higher target affinity and low toxicity. In addition, {RNA-cleaving} ribozymes and deoxyribozymes, and the use of 21-mer double-stranded {RNA} molecules for {RNA} interference applications in mammalian cells offer highly efficient strategies to suppress the expression of a specific gene.},
	number = {8},
	journal = {European Journal of Biochemistry},
	author = {Kurreck, Jens},
	month = apr,
	year = {2003},
	keywords = {antisense‐oligonucleotides, deoxyribozymes, {DNA} enzymes, locked nucleic acids, peptide nucleic acids, phosphorothioates, ribozymes, {RNA} interference, small interfering {RNA}},
	pages = {1628--1644}
},

@article{legant_measurement_2010-1,
	title = {Measurement of mechanical tractions exerted by cells in three-dimensional matrices},
	volume = {7},
	issn = {1548-7105},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/21076420},
	doi = {10.1038/nmeth.1531},
	abstract = {Quantitative measurements of cell-generated forces have heretofore required that cells be cultured on two-dimensional substrates. We describe a technique to quantitatively measure three-dimensional traction forces exerted by cells fully encapsulated in well-defined elastic hydrogel matrices. Using this approach we measured traction forces for several cell types in various contexts and revealed patterns of force generation attributable to morphologically distinct regions of cells as they extend into the surrounding matrix.},
